package com.openske.rules.software;

import com.openske.model.software.Vulnerability;
import com.openske.model.software.Weakness;
import com.openske.engine.Engine;

rule "Declare the corresponding weaknesses for the found vulnerabilities"
    when
        $vuln : Vulnerability( occurrences.size > 0 )
    then
        Engine.insertFacts(Weakness.forVulnerability($vuln));
end
